    "react-dom": "^18",
    "react-transition-group": "^4.4.5",
    "slugify": "^1.6.6",
    "three": "^0.158.0",
    "wildcard-match": "^5.1.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.7.2",
    "@types/fontfaceobserver": "^2.1.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-transition-group": "^4.4.9",
    "@types/three": "^0.158.2",
    "autoprefixer": "^10.0.1",
